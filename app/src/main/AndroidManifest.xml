<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mrj.morejmoney">

    <application
        android:name="com.mrj.morejmoney.ui.pi.PIApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.mrj.morejmoney.ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mrj.morejmoney.ui.MainActivity" />
        <activity android:name="com.mrj.morejmoney.ui.signin.SigninActivity" />
        <activity android:name="com.mrj.morejmoney.ui.recover.RecoverActivity"></activity>
        <receiver android:name="com.mrj.morejmoney.ui.AlarmReceiver"/>
    </application>

</manifest>